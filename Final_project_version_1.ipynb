   "cell_type": "code",
   "source": [
    "def V(x, alpha):\n",
    "    return alpha*x**4 - 2*x**2 + 1/alpha"
   ],
   "id": "782b758e90922440",
   "outputs": [],
   "execution_count": 5
  },
  {
   "metadata": {
    "ExecuteTime": {
